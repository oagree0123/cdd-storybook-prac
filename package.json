{
  "name": "otory-book",
  "version": "1.0.0",
  "description": "storybook을 활용한 컴포넌트 주도 개발 연습",
  "author": "oagree0123",
  "license": "MIT",
  "keywords": [
    "storybook",
    "react"
  ],
  "scripts": {
    "build": "npx rimraf dist && rollup -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
